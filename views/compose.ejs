<%- include("partials/header") %>

<h1>Compose</h1>
<form action="/" method="post">
  <div class="form-group">
      <label>Title</label>
      <input type="text" class="form-control" autocomplete="off" name="postTitle"> <br>
      <label>Publish</label>
      <textarea class="form-control" rows="5" autocomplete="off" name="postContent"></textarea>
  </div>
  <button type="submit" class="btn btn-primary">Publish</button>
</form>



